<launch>
    <arg name="source" default="/camera_raw"/>
    <include file="$(find sensors)/launch/camera.launch"/>
    <node pkg="yolov8_ros" name="yolo" type="detectv8bags.py" output="screen">
        <param name="source" value="$(arg source)"/>
    <node pkg="rosbag" type="record" name="bounding_box_recorder" args="record -O bounding_boxes.bag /BoundingBoxes"/>
</launch>
